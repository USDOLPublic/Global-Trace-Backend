{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".e2e-spec.ts$",
    "testTimeout": 60000,
    "forceExit": true,
    "moduleNameMapper": {
        "^~(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "maxConcurrency": 4,
    "maxWorkers": 4
}