<mj-head>
    <mj-font name="Roboto" href="https://fonts.googleapis.com/css?family=Roboto" />
    <mj-font name="Mukta" href="https://fonts.googleapis.com/css?family=Mukta" />
    <mj-font name="Montserrat" href="https://fonts.googleapis.com/css?family=Montserrat" />
    <mj-style inline="inline">
        h1 { text-transform: uppercase; } .usdol-font * { font-family:"Roboto", "Mukta", "Montserrat", sans-serif; line-height: 20px; font-size: 13px; color: #000 }
        .usdol-blue { color: #226fea; } .usdol-font h1 { color: #226fea; text-transform: uppercase; text-align: center;
        font-size: 36px; font-weight: normal; } .usdol-link a { color: #226fea; } .usdol-button td[role=presentation] {
        border-radius: 4px !important; background-color: #150A84 !important; } .usdol-button a { background: transparent
        !important; font-family:"Roboto", "Mukta", "Montserrat", sans-serif !important; font-size: 14px !important; font-weight: 500 !important; line-height: 14px; padding: 13px 25px !important; } .usdol-table-header { color: #226fea; text-align: right;
        padding-right: 3em; min-width: 100px; vertical-align: top; padding-top: 1em; padding-bottom: 1em; }
        .usdol-table-right { padding-top: 1em; padding-bottom: 1em; } .usdol-upload-required { color: #4c4c4c; text-align:
        center; font-size: 18px; } .usdol-section-table-left { padding-right: 1em; text-align: left; }
